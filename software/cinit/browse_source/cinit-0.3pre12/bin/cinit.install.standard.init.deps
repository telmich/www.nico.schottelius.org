#!/bin/sh
# Nico Schottelius
# cinit: add standard sub-configuration directories to init wants
# Date: 2005-10-11
# 

. $(dirname $0)/cinit.read-conf

$(dirname $0)/cinit.add.dependency init wants getty
$(dirname $0)/cinit.add.dependency init wants local-services
$(dirname $0)/cinit.add.dependency init wants local-tuning
$(dirname $0)/cinit.add.dependency init wants mount
$(dirname $0)/cinit.add.dependency init wants network

exit 0
